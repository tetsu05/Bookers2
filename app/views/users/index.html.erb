<%= render 'books/book',user: @user %>
<%= render 'books/newbook',book: @book %>
<h2>users</h2>
<% @users.each do |user| %>
  <p>image</p>
  <span><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></span>
  <p>name</p>
  <span><%= user.name %></span>
  <p><%= link_to "Show", user_path(user) %></p>
<% end %>
<footer>
  <div class="container mt-5">
  	<div class="row">
    	<div class="mx-auto">
    		<p>CopyRight Infratop.inc</p>
    	</div>
    </div>
  </div>
</footer>